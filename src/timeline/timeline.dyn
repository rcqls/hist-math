tL.f.drawPopupEvents=function() {

	var html = tL.popup.events.div;
	html.id = 'eventpopup';
	html.style.minWidth = "100px";
	html.style.minHeight = "50px";
	html.style.opacity= 0.75;
	html.style.paddingRight=html.style.paddingLeft= "10px";
